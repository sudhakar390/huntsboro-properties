<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="PropertyAdminPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>org.huntsboro.data.entity.ResidenceAndRentalHistory</class>
    <class>org.huntsboro.data.entity.ApplicantsAndTenants</class>
    <class>org.huntsboro.data.entity.ApplicationsAndLeases</class>
    <class>org.huntsboro.data.entity.EmploymentIncomeHistory</class>
    <class>org.huntsboro.data.entity.Properties</class>
    <class>org.huntsboro.data.entity.References</class>
    <class>org.huntsboro.data.entity.RentalOwners</class>
    <class>org.huntsboro.data.entity.Units</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/propertyadmin?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="huntdetg_wdg"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="wdg123"/>
    </properties>
  </persistence-unit>
</persistence>
 
